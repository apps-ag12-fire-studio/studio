rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {

    // Permite que qualquer pessoa leia e escreva na coleção 'submittedContracts'
    // IMPORTANTE: Para produção, restrinja essas regras!
    match /submittedContracts/{document=**} {
      allow read, write: if true;
    }

    // Adicione aqui regras para outras coleções, se necessário.
    // Por padrão, todas as outras coleções serão negadas.
  }
}
